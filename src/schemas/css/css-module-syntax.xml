<?xml version="1.0" encoding="utf-8"?>
<CssModule name="Syntax" standard-reference="http://www.w3.org/TR/css3-syntax/" status="WD" pub-date="2003-08-13">

  <CssAtDirective name="charset" version="1.0" browsers="all" restriction="string" syntax="@charset 'utf-8';" description="Defines character set of the document." standard-reference="http://www.w3.org/TR/css3-syntax/#charset" />

  <CssAtDirective name="import" version="1.0" browsers="all" syntax="@import url('file.css') tv, print;" description="Includes content of another file." standard-reference="http://www.w3.org/TR/css3-syntax/#at-import" />

  <CssAtDirective name="media" version="2.0" browsers="all" description="Defines a stylesheet for a particular media type." syntax="@media print { ... }" standard-reference="http://www.w3.org/TR/css3-mediaqueries" />

  <CssAtDirective name="host" version="2.0" browsers="C" description="Applies to elements specified in shadow trees." syntax="@host { :scope { color: red; } }" standard-reference="http://www.w3.org/TR/shadow-dom/#host-at-rule" />

  <CssAtDirective name="supports" version="3.0" browsers="C,FF,O" description="A conditional group rule whose condition tests whether the user agent supports CSS property:value pairs." syntax="@supports (display: flexbox) { div { display: flexbox; } }" standard-reference="http://www.w3.org/TR/css3-conditional/#at-supports" />

  <CssPropertyValue type="media" version="2.0">
    <entry version="2.0" value="all" />
    <entry version="2.0" value="braille" />
    <entry version="2.0" value="embossed" />
    <entry version="2.0" value="handheld" />
    <entry version="2.0" value="print" />
    <entry version="2.0" value="projection" />
    <entry version="2.0" value="screen" />
    <entry version="2.0" value="speech" />
    <entry version="2.0" value="tty" />
    <entry version="2.0" value="tv" />
  </CssPropertyValue>
</CssModule>