<CssModule
	name="Namespaces"
	standard-reference="http://www.w3.org/TR/css3-namespace/"
	status="REC"
	pub-date="2011-09-29">
	<CssAtDirective
		name="namespace"
		version="3.0"
		browsers="C,FF1,IE9,O8,S1"
		syntax="@namespace [prefix] string|url;"
		description="Declares a prefix and associates it with a namespace name."
		standard-reference="http://www.w3.org/TR/css3-namespace/#declaration" />
</CssModule>