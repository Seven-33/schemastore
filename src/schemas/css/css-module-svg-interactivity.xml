<CssModule
	name="SVG Interactivity"
	standard-reference="http://www.w3.org/TR/SVG11/interact.html"
	status="REC"
	pub-date="2011-08-16">
	<CssProperty
		name="pointer-events"
		version="3.0"
		restriction="enum"
		description="Allows authors to control under what circumstances (if any) a particular graphic element can become the target of mouse events."
		standard-reference="http://www.w3.org/TR/SVG11/interact.html#PointerEventsProperty">
		<entry
			value="auto" />
		<entry
			value="none" />
		<entry
			value="visiblePainted" />
		<entry
			value="visibleFill" />
		<entry
			value="visibleStroke" />
		<entry
			value="visible" />
		<entry
			value="painted" />
		<entry
			value="fill" />
		<entry
			value="stroke" />
		<entry
			value="all" />
	</CssProperty>
</CssModule>